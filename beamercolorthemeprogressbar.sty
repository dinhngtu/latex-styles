% Some color definitions...
%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%


\definecolor{progressbar@fgblack}{rgb}{0.2, 0.3, 0.5}
\definecolor{progressbar@bgwhite}{rgb}{1, 1, 1}

\definecolor{progressbar@fgred}{rgb}{0.9, 0.25, 0.25}

\definecolor{progressbar@darkbgblue}{rgb}{0.01,0.05,0.15}
\definecolor{progressbar@darkfgblue}{rgb}{0.25,0.45,0.5}

\mode<presentation>

\setbeamercolor{palette primary}{fg=progressbar@fgblack,bg=progressbar@bgwhite}
\setbeamercolor{palette secondary}{fg=progressbar@fgblack,bg=progressbar@bgwhite}
\setbeamercolor{palette tertiary}{fg=progressbar@fgblack,bg=progressbar@bgwhite}
\setbeamercolor{palette quaternary}{fg=progressbar@fgblack,bg=progressbar@bgwhite}

\setbeamercolor{structure}{fg=progressbar@fgblack,bg=progressbar@bgwhite}

\setbeamercolor{background canvas}{parent=palette primary}

\setbeamercolor{section in head/foot}{parent=palette secondary}
\setbeamercolor{frametitle}{parent=palette primary}

\setbeamercolor{normal text}{parent=palette primary}
\setbeamercolor{alerted text}{fg=black!10!red,bg=bg}
\setbeamercolor{example text}{fg=black!40!green,bg=bg}
\setbeamercolor{gblock text}{fg=black!40!green,bg=bg}

\setbeamercolor{block title}{use=structure,fg=structure.fg,bg=progressbar@bgwhite}
\setbeamercolor{block title alerted}{fg=alerted text.fg,bg=alerted text.fg!10!bg}
\setbeamercolor{block title example}{fg=example text.fg,bg=example text.fg!10!bg}
\setbeamercolor{block title gblock}{fg=gblock text.fg,bg=gblock text.fg!10!bg}

\setbeamercolor{block body}{fg=white,bg=progressbar@bgwhite}
\setbeamercolor{block body alerted}{fg=alerted text.fg,bg=alerted text.fg!10!bg}
\setbeamercolor{block body example}{fg=example text.fg,bg=example text.fg!10!bg}
\setbeamercolor{block body gblock}{fg=gblock text.fg,bg=gblock text.fg!10!bg}

\setbeamercolor{title}{fg=progressbar@fgred,bg=progressbar@bgwhite}
\setbeamercolor{subtitle}{parent=title}

\setbeamercolor{item}{progressbar@darkfgblue}


\mode
<all>



\makeatother
